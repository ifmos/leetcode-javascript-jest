{
  "name": "leetcode-javascript-jest",
  "version": "0.0.0",
  "description": "Starter kit with zero-config for testing LeetCode answer in JavaScript, supported by jest, recommend working with vscode!",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "npm run test:watch"
  },
  "dependencies": {
    "jest": "^24.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "\\.(test|spec)\\.js$",
    "moduleFileExtensions": [
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.js"
    ]
  }
}
